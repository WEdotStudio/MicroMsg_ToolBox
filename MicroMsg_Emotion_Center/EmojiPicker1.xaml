                    <Image x:Name="p10" Grid.Row="3" Grid.Column="0" Source="/Res/blue_face/10.png" Tap="Button_Click_1"/>
                    <Image x:Name="p11" Grid.Row="3" Grid.Column="1" Source="/Res/blue_face/11.png" Tap="Button_Click_1"/>
                    <Image x:Name="p12" Grid.Row="3" Grid.Column="2" Source="/Res/blue_face/12.png" Tap="Button_Click_1"/>
                    <Image x:Name="p13" Grid.Row="4" Grid.Column="0" Source="/Res/blue_face/13.png" Tap="Button_Click_1"/>
